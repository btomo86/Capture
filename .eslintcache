ght: 90vh;\n  padding-top: 20vh;\n  position: relative;\n  h2 {\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    transform: translate(-50%, -10%);\n  }\n  img {\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n  }\n`;\n\nconst Awards = styled.div`\n  min-height: 80vh;\n  display: flex;\n  margin: 5rem 10rem;\n  align-items: center;\n  justify-content: space-around;\n`;\n\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    height: 0.5rem;\n    margin: 1rem 0rem;\n  }\n  p {\n    padding: 2rem 0rem;\n  }\n`;\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\n\nexport default MovieDetail;\n","/Users/benthompson/Projects/capture-portfolio/src/movieState.js",[],"/Users/benthompson/Projects/capture-portfolio/src/animation.js",[],"/Users/benthompson/Projects/capture-portfolio/src/components/Wave.js",[],"/Users/benthompson/Projects/capture-portfolio/src/components/Toggle.js",[],"/Users/benthompson/Projects/capture-portfolio/src/components/UseScroll.js",[],"/Users/benthompson/Projects/capture-portfolio/src/components/useScroll.js",[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":11,"column":18,"nodeType":"89","messageId":"90","endLine":11,"endColumn":27},"no-native-reassign",["91"],"no-negated-in-lhs",["92"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]