ace-around;\n`;\n\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    height: 0.5rem;\n    margin: 1rem 0rem;\n  }\n  p {\n    padding: 2rem 0rem;\n  }\n`;\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\n\nexport default MovieDetail;\n","/Users/benthompson/Projects/capture-portfolio/src/movieState.js",[],"/Users/benthompson/Projects/capture-portfolio/src/animation.js",[],"/Users/benthompson/Projects/capture-portfolio/src/components/Wave.js",[],"/Users/benthompson/Projects/capture-portfolio/src/components/Toggle.js",[],"/Users/benthompson/Projects/capture-portfolio/src/components/UseScroll.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":5,"column":31,"nodeType":"86","messageId":"87","endLine":5,"endColumn":51},{"ruleId":"84","severity":1,"message":"88","line":11,"column":18,"nodeType":"86","messageId":"87","endLine":11,"endColumn":27},"no-native-reassign",["89"],"no-negated-in-lhs",["90"],"no-unused-vars","'anitmateSharedLayout' is defined but never used.","Identifier","unusedVar","'setMovies' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]